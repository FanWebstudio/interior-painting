{
  "extends": "astro/tsconfigs/base",
  "include": [
    ".astro/types.d.ts",
    "src/types/**/*.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "typeRoots": ["./node_modules/@types", "./src/types"],
    "skipLibCheck": true,
    "noImplicitAny": false,
    "allowJs": true,
    "jsx": "preserve"
  }
}
